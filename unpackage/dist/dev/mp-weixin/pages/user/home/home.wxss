
page {
	background: #fff;
}
.home {
	border-top: 2rpx solid #E5E5E5;
	margin-top: 10rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
	padding: 0 30rpx;
	padding-top: 36rpx;
	color: #fff;
}
.dispatch {
	width: 48%;
	background: -webkit-linear-gradient(308deg, #5AA5FF 0%, #343CFE 100%);
	background: linear-gradient(142deg, #5AA5FF 0%, #343CFE 100%);
	-webkit-box-shadow: 0px 4rpx 8rpx 0px #4A77FF;
	        box-shadow: 0px 4rpx 8rpx 0px #4A77FF;
	border-radius: 16rpx;
	position: relative;
}
.dispatch-title {
	font-size: 44rpx;
	padding: 32rpx 38rpx;
}
.dispatch-img {
	width: 100%;
	position: absolute;
	bottom: 34rpx;
	padding: 0 40rpx 0 34rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
}
.dispatch-img-left {
	width: 56rpx;
	height: 56rpx;
}
.dispatch-img-right {
	width: 90rpx;
	height: 90rpx;
}
.home-right {
	width: 48%;
}
.query {
	width: 100%;
	background: -webkit-linear-gradient(314deg, #FFC820 0%, #FFA300 100%);
	background: linear-gradient(136deg, #FFC820 0%, #FFA300 100%);
	-webkit-box-shadow: 0px 4rpx 8rpx 0px #FAB70B;
	        box-shadow: 0px 4rpx 8rpx 0px #FAB70B;
	border-radius: 16rpx;
	position: relative;
}
.query-title {
	font-size: 36rpx;
	padding: 42rpx 28rpx;
}
.query-img {
	width: 100%;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	padding: 0 32rpx 0 36rpx;
	position: absolute;
	bottom: 24rpx;
}
.query-img-left {
	width: 42rpx;
	height: 42rpx;
}
.query-img-right {
	width: 58rpx;
	height: 68rpx;
}
.query-img-rights {
	width: 68rpx;
	height: 56rpx;
}
.complaint {
	background: -webkit-linear-gradient(315deg, #00FFFA 0%, #00CACE 100%);
	background: linear-gradient(135deg, #00FFFA 0%, #00CACE 100%);
	-webkit-box-shadow: 0px 4rpx 8rpx 0px #00CDD1;
	        box-shadow: 0px 4rpx 8rpx 0px #00CDD1;
	border-radius: 16rpx;
	margin-top: 30rpx;
}
	

