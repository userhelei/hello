<view><cu-tab-bar class="c-black" vue-id="6cab0655-1" bgColor="bg-white" isBack="{{true}}" onVueInit="__l"><view slot="backText"></view><view slot="content">接单</view></cu-tab-bar><view class="appointment"><view class="appointment-list" a:for="{{appointmentList}}" a:for-item="item" a:for-index="index" a:key="index"><view class="appointment-list-line"><view class="list-line-left">工单编号:</view><view class="list-line-right">{{item.ordercode}}</view></view><view class="appointment-list-line mt-30"><view class="list-line-left">问题详情:</view><view class="list-line-right list-line-rights">{{''+item.questioncontent+''}}</view></view><view class="appointment-list-line mt-30"><view class="list-line-left">派单时间:</view><view class="list-line-right">{{item.allocatetime}}</view></view><view data-event-opts="{{[['tap',[['toDetails',['$0'],[[['appointmentList','',index,'id']]]]]]]}}" class="appointment-btn-details" onTap="__e">查看详情</view></view></view><block a:if="{{appointmentList.length==0}}"><view class="text-center mt-100">暂无订单</view></block></view>