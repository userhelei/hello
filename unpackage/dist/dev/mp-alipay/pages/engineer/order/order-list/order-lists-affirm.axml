<view><cu-tab-bar class="c-black" vue-id="2221d3b8-1" bgColor="bg-white" isBack="{{true}}" onVueInit="__l"><view slot="backText"></view><view slot="content">确认工单</view></cu-tab-bar><view class="subscribe"><view class="subscribe-name subscribe-names"><view class="subscribe-left subscribe-lefts">工单编号:</view><view>{{orderNum}}</view></view><view class="subscribe-name mt-20"><view class="subscribe-left">完成情况:</view><textarea class="subscribe-right subscribe-right-area" placeholder="请输入完成情况" data-event-opts="{{[['input',[['__set_model',['','centent','$event',[]]]]]]}}" value="{{centent}}" onInput="__e"></textarea></view></view><view class="subscribe mt-30"><view class="subscribe-name"><view class="subscribe-left">图片上传:</view><view class="grid col-4 grid-square flex-sub" style="width:75%;"><view class="bg-img" data-url="{{imgList[index]}}" data-event-opts="{{[['tap',[['ViewImage',['$event']]]]]}}" onTap="__e" a:for="{{imgList}}" a:for-item="item" a:for-index="index" a:key="index"><image src="{{imgList[index]}}" mode="aspectFill"></image><view class="cu-tag bg-red" data-index="{{index}}" data-event-opts="{{[['tap',[['DelImg',['$event']]]]]}}" catchTap="__e"><text class="cuIcon-close"></text></view></view><block a:if="{{imgList.length<4}}"><view data-event-opts="{{[['tap',[['ChooseImage',['$event']]]]]}}" class="solids" onTap="__e"><text class="cuIcon-cameraadd"></text></view></block></view></view></view><view data-event-opts="{{[['tap',[['toSubmit',['$event']]]]]}}" class="subscribe-btn" onTap="__e">提交</view></view>